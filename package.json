{
  "name": "viwiserver",
  "version": "0.0.3",
  "description": "A Reactive Extensions based server for the Volkswagen Infotainment Web Interface as public in 2016 by W3C",
  "main": "index.ts",
  "scripts": {
    "test": "node ./node_modules/jasmine/bin/jasmine.js jasmine.json",
    "watch": "node ./node_modules/nodemon/bin/nodemon.js cli.js",
    "debug": "node --inspect --debug-brk ./bin/cli.js",
    "serve": "node ./bin/cli.js",
    "start": "npm run serve",
    "build:docker": "docker build -t viwi-server ."
  },
  "author": "Dr. Patrick Bartsch",
  "license": "MIT",
  "dependencies": {
    "@reactivex/rxjs": "^5.0.2",
    "body-parser": "^1.15.2",
    "command-line-args": "^3.0.5",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "express": "^4.14.0",
    "http": "0.0.0",
    "uuid": "^3.0.1",
    "winston": "^2.3.1",
    "ws": "^1.1.1"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/compression": "0.0.33",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.0.34",
    "@types/jasmine": "^2.5.43",
    "@types/node": "^6.0.65",
    "@types/request": "0.0.39",
    "@types/uuid": "^2.0.29",
    "@types/winston": "^2.2.0",
    "@types/ws": "0.0.37",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-conventional-changelog": "^1.1.0",
    "gulp-file": "^0.3.0",
    "gulp-jasmine": "^2.4.2",
    "gulp-nodemon": "^2.2.1",
    "gulp-typescript": "^3.1.3",
    "request": "^2.79.0",
    "typescript": "^2.1.4"
  }
}